{
  "name": "run/laravel-apollo-sync",
  "description": "apollo sync",
  "keywords": [
    "Apollo",
    "Client",
    "Laravel"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/run6/laravel-apollo-sync",
  "require": {
    "php": "~7.0",
    "illuminate/console": "^5.2|^6",
    "illuminate/support": "^5.2|^6"
  },
  "autoload": {
    "psr-4": {
      "Run\\ApolloSync\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.0|^4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Run\\ApolloSync\\ApolloSyncServiceProvider"
      ]
    }
  }
}
